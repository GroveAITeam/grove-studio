{
  "name": "web",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0",
    "pnpm": ">=7.33.7"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "node scripts/lint-fix.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.4",
    "@vueuse/core": "^13.1.0",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "husky": "^7.0.4",
    "lucide-vue-next": "^0.501.0",
    "pinia": "^3.0.2",
    "reka-ui": "^2.2.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.4",
    "tw-animate-css": "^1.2.8",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@types/node": "^22.14.1",
    "@vitejs/plugin-vue": "^5.2.2",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.24.0",
    "eslint-plugin-format": "^1.0.1",
    "lint-staged": "^15.5.1",
    "sass-embedded": "^1.87.0",
    "typescript": "~5.7.2",
    "vite": "^6.3.1",
    "vue-tsc": "^2.2.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{vue,js,ts,jsx,tsx}": [
      "npm run lint:fix",
      "vue-tsc --noEmit",
      "git add ."
    ]
  }
}
